{
	"expo": {
		"name": "splitly",
		"slug": "splitly",
		"version": "1.0.0",
		"orientation": "portrait",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"web": {
			"bundler": "metro",
			"output": "static"
		},
		"plugins": ["expo-router", ["expo-splash-screen"]],
		"experiments": {
			"typedRoutes": true
		}
	}
}
